         <div class="tab-pane fade" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab">
           <section class="container ">
             <div class="table-responsive"  style="width: max-content;">
                  <table class="table mb-5">
                      <thead class="bg-yellow-200">
                          <tr>
   
                              <th>Order Id</th>
                              <th>Date Purchased</th>
                              <th>Payment</th>
                              <th>Order Status</th>
                              <th>Action</th>
                              <th>Orders</th>
															
                          </tr>
                      </thead>
                      <% for(let key of userorders) { %>
                      <tbody>
                          <td><%=key._id%></td>
                          <td><%=key. createdAt.toDateString ()%></td>
                          <td><%=key.payment%></td>
                          <td><%=key.status%></td>
                          <td>
														<% if (key.status === 'Order Return') { %>
															<a href="/returnOrder?id=<%= key._id %>" class="btn btn-warning disabled-links">Return</a>
															<style>
																.disabled-links {
																	pointer-events: none;
																	opacity: 0.5;
																}
															</style>
														<% } else if (key.status === 'Canceled') { %>
															<a href="/cancelOrder?id=<%= key._id %>" class="btn btn-danger disabled-link">Cancel</a>
															<style>
																.disabled-link {
																	pointer-events: none;
																	opacity: 0.5;
																}
															</style>
														<% } else if (key.status === 'Delivered') { %>
															<% const currentDate = new Date();
																 const deliveredDate = new Date(key.updatedAt);
																 const daysElapsed = (currentDate - deliveredDate) / (1000 * 60 * 60 * 24);
																 if (daysElapsed > 14) { %>
																	 <a href="/returnOrder?id=<%= key._id %>" class="btn btn-warning disabled-links">Return</a>
																	 <style>
																		 .disabled-links {
																			 pointer-events: none;
																			 opacity: 0.5;
																		 }
																	 </style>
																 <% } else { %>
																	 <a href="/returnOrder?id=<%= key._id %>" class="btn btn-warning">Return</a>
																 <% } %>
														<% } else { %>
															<a href="/cancelOrder?id=<%= key._id %>" class="btn btn-danger">Cancel</a>
															<style>
																.disabled-links {
																	pointer-events: none;
																	opacity: 0.5;
																}
															</style>
														<% } %>
													</td>
													
                         
                          <td>
                              <a href="/userViewOrder?id=<%=key._id%>" class="btn btn-primary">View details</a>
                           </td>
													 
                           
                      </tbody>
                      <% } %>
                  </table>
               </div>
          </section>
